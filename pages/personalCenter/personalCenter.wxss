/* pages/personalCenter/personalCenter.wxss */
.center {
  background: #fff;
  position: relative;
}
.geren-zhong-xin,.geren-zhong-xin-image{
  width: 682rpx;
  height: 266rpx;
  margin: 0 auto;
}
.muem-li {
  padding: 34rpx;
  border-bottom:1rpx solid rgba(220,220,220,1);
}
.muem-li image{
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
}
.muem-li view{
  display: inline-block;
  vertical-align: middle;
  font-size:32rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  padding-left: 40rpx;
}
.center-float-image{
  width:98rpx;
  height:98rpx;
  border-radius: 100%;
    /* display: inline-block; */
  /* vertical-align: middle; */
  /* margin-top: 20rpx; */
  float: left;
}
.center-float{
  position: absolute;
  left: 64rpx;
  top: 30rpx;
}
.center-float-l {
  float: left;
  margin-top: 40rpx;
  margin-left: 20rpx;
  font-size:28rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(254,254,254,1);
}